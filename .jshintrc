{
  "bitwise":true, // Disable ^ &
  "camelcase":true,//Hump userName || USER_NAME
  "curly":true,// if,while new block or scope need {}
  "eqeqeq":true,// force === !==
  "forin":true,//obj.hasOwnProperty(key)
  "es3": true, //
  "freeze":true,//forbidden Extending prototype of native object
  "indent": 2,
  "latedef": true, // true: Require variables/functions to be defined before being used
  "unused":true,//var variables must be used
  "maxparams":3,//max number of params
  "maxdepth":3,//max depth of {}
  "loopfunc":false// forbidden for --> function
}
